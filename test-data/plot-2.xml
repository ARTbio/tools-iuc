<catchall>
    <image>
        <bcolor>#afffaf</bcolor>
    </image>
    <ideogram>
        <ideo_radius>1.0</ideo_radius>
        <ideo_thickness>0.1</ideo_thickness>
        <band_stroke_color>#0000ff</band_stroke_color>
        <band_stroke_thickness>1.0</band_stroke_thickness>
        <band_stroke_transparency>1.0</band_stroke_transparency>
        <ideo_stroke_color>#000000</ideo_stroke_color>
        <ideo_stroke_thickness>1.0</ideo_stroke_thickness>
        <ideo_tag_label_with_tag>false</ideo_tag_label_with_tag>

        <ideo_fill>true</ideo_fill>
        <ideo_fill_bands>false</ideo_fill_bands>
        <ideo_fill_color>#ff00ff</ideo_fill_color>
        <ideo_label_case>lower</ideo_label_case>
        <ideo_label_center>true</ideo_label_center>
        <ideo_label_font>serif</ideo_label_font>
        <ideo_label_parallel>true</ideo_label_parallel>
        <ideo_label_radius>1.0</ideo_label_radius>
        <ideo_label_size>1.0</ideo_label_size>
        <plot>
            <plot_glyph>rectangle</plot_glyph>
            <plot_stroke_color>#ff0000</plot_stroke_color>
            <plot_r1>0.75</plot_r1>
            <plot_r0>0.9</plot_r0>
            <plot_max>1.0</plot_max>
            <plot_color>#ff0000</plot_color>
            <plot_glyph_size>1.0</plot_glyph_size>
            <plot_show>Yes</plot_show>
            <plot_min>0.0</plot_min>
            <plot_stroke_thickness>1.0</plot_stroke_thickness>

            <datatype>wig</datatype>
            <plot_file>test-data/miro.wig</plot_file>
            <type>scatter</type>
        </plot>
    </ideogram>
</catchall>
