<?xml version="1.0"?>
<application id="patmatdb">
  <metadata>
    <documentation><![CDATA[Search protein sequences with a sequence motif]]></documentation>
    <groups><![CDATA[Protein:Motifs]]></groups>
    <relations><![CDATA[EDAM_topic:0158 Sequence motifs]]></relations>
    <relations><![CDATA[EDAM_operation:3087 Protein site detection]]></relations>
    <relations><![CDATA[EDAM_operation:0347 Sequence database search
                (by motif or pattern)]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[Protein]]></type>
      <relations><![CDATA[EDAM_data:2886 Sequence record (protein)]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="string" name="motif">
      <parameter><![CDATA[Y]]></parameter>
      <minlength><![CDATA[2]]></minlength>
      <information><![CDATA[Protein motif to search for]]></information>
      <help><![CDATA[Patterns for patmatdb are based on the format of pattern
           used in the PROSITE database. For example:
           '[DE](2)HS{P}X(2)PX(2,4)C' means two Asps or Glus in any order
           followed by His, Ser, any residue other then Pro, then two of any
           residue followed by Pro followed by two to four of any residue
           followed by Cys. The search is case-independent, so 'AAA' matches
           'aaa'.]]></help>
      <knowntype><![CDATA[prosite pattern]]></knowntype>
      <relations><![CDATA[EDAM_data:2071 Sequence motif (protein)]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="report" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <rformat><![CDATA[dbmotif]]></rformat>
      <precision><![CDATA[0]]></precision>
      <relations><![CDATA[EDAM_data:1298 Sequence features (motifs)]]></relations>
    </parameter>
  </section>
</application>
