<?xml version="1.0"?>
<application id="inforesidue">
  <metadata>
    <documentation><![CDATA[Return information on a given amino acid residue]]></documentation>
    <groups><![CDATA[Data retrieval:Chemistry data]]></groups>
    <gui><![CDATA[yes]]></gui>
    <batch><![CDATA[yes]]></batch>
    <cpu><![CDATA[low]]></cpu>
    <relations><![CDATA[EDAM_topic:0154 Small molecules]]></relations>
    <relations><![CDATA[EDAM_operation:2420 Analysis and processing]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="string" name="code">
      <parameter><![CDATA[Y]]></parameter>
      <information><![CDATA[Amino acid residue code]]></information>
      <help><![CDATA[Enter IUPAC amino acids codes, e.g. A, C, D, E etc.]]></help>
      <knowntype><![CDATA[protein codes]]></knowntype>
      <default><![CDATA[ABCDEFGHIJKLMNOPQRSTUVWXYZ]]></default>
      <relations><![CDATA[EDAM_identifier:0994 Amino acid identifier]]></relations>
    </parameter>
    <parameter type="datafile" name="aadata">
      <information><![CDATA[Amino acids properties and molecular weight data file]]></information>
      <help><![CDATA[Amino acid properties]]></help>
      <default><![CDATA[Eamino.dat]]></default>
      <knowntype><![CDATA[amino acid properties]]></knowntype>
      <relations><![CDATA[EDAM_data:1502 Amino acid index (chemical classes)]]></relations>
    </parameter>
    <parameter type="datafile" name="mwdata">
      <information><![CDATA[Molecular weights data file]]></information>
      <help><![CDATA[Molecular weight data for amino acids]]></help>
      <default><![CDATA[Emolwt.dat]]></default>
      <knowntype><![CDATA[molecular weights]]></knowntype>
      <relations><![CDATA[EDAM_data:1505 Amino acid index (molecular weight)]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[inforesidue output]]></knowntype>
      <relations><![CDATA[EDAM_data:0894 Amino acid annotation]]></relations>
    </parameter>
  </section>
</application>
