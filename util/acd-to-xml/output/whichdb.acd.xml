<?xml version="1.0"?>
<application id="whichdb">
  <metadata>
    <documentation><![CDATA[Search all sequence databases for an entry and retrieve it]]></documentation>
    <groups><![CDATA[Data retrieval:Sequence data]]></groups>
    <gui><![CDATA[yes]]></gui>
    <batch><![CDATA[yes]]></batch>
    <cpu><![CDATA[low]]></cpu>
    <relations><![CDATA[EDAM_topic:0090 Data search and retrieval]]></relations>
    <relations><![CDATA[EDAM_operation:0344 Sequence retrieval (by code)]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="string" name="entry">
      <parameter><![CDATA[Y]]></parameter>
      <information><![CDATA[ID or Accession number]]></information>
      <knowntype><![CDATA[sequence id input]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1063 Sequence identifier]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="toggle" name="get">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Retrieve sequences]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="showall">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Show failed attempts]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="outfile" name="outfile">
      <standard><![CDATA[@(!$(get))]]></standard>
      <nullok><![CDATA[$(get)]]></nullok>
      <nulldefault><![CDATA[$(get)]]></nulldefault>
      <knowntype><![CDATA[whichdb output]]></knowntype>
      <relations><![CDATA[EDAM_data:0957 Database metadata]]></relations>
    </parameter>
  </section>
</application>
