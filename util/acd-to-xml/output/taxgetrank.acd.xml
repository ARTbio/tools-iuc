<?xml version="1.0"?>
<application id="taxgetrank">
  <metadata>
    <documentation><![CDATA[Get parents of taxon(s)]]></documentation>
    <groups><![CDATA[Ontology:Taxonomy]]></groups>
    <relations><![CDATA[EDAM_topic:0637 Taxonomy]]></relations>
    <relations><![CDATA[EDAM_operation:2422 Data retrieval]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="taxon" name="taxons">
      <default><![CDATA[]]></default>
      <parameter><![CDATA[Y]]></parameter>
      <relations><![CDATA[EDAM_identifier:1875 NCBI taxon]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="list" name="rank">
      <parameter><![CDATA[Y]]></parameter>
      <default><![CDATA[kingdom,phylum,class,order,family,genus,species]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[28]]></maximum>
      <header><![CDATA[Taxonomic rank(s)]]></header>
      <values><![CDATA[kingdom:Kingdom;
    	    phylum:Phylum;
	    class:Class;
	    order:Order;
	    family:Family;
	    genus:Genus;
	    species:Species;
	    tribe:Tribe;
	    varietas:varietas;
	    species group:Species group;
	    species subgroup:Species subgroup;
	    no rank:None;
	    superkingdom:Superkingdom;
	    superphylum:Superphylum;
	    superclass:Superclass;
	    superorder:Superorder;
	    superfamily:Superfamily;
    	    subkingdom:Subkingdom;
    	    subphylum:Subphylum;
    	    subclass:Subclass;
	    suborder:Suborder;
	    subfamily:Subfamily;
	    subgenus:Subgenus;
	    subspecies:Subspecies;
	    subtribe:Subtribe;
	    infraclass:Infraclass;
	    infraorder:Infraorder;
	    parvorder:Parvorder;]]></values>
      <delimiter><![CDATA[;]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Find taxons at rank]]></information>
      <help><![CDATA[Multiple ranks can be specified, or '*' for all]]></help>
      <relations><![CDATA[EDAM_identifier:1872 Taxonomic classification]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="hidden">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Show taxons hidden in GenBank]]></information>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outtaxon" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <relations><![CDATA[EDAM_data:2858 Ontology concept]]></relations>
    </parameter>
  </section>
</application>
