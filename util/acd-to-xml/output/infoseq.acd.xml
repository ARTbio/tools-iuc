<?xml version="1.0"?>
<application id="infoseq">
  <metadata>
    <documentation><![CDATA[Display basic information about sequences]]></documentation>
    <groups><![CDATA[Data retrieval:Sequence data]]></groups>
    <relations><![CDATA[EDAM_topic:0090 Data search and retrieval]]></relations>
    <relations><![CDATA[EDAM_operation:1813 Sequence retrieval]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapany]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="columns">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Print information in neat, aligned columns.]]></information>
      <help><![CDATA[Set this option on (Y) to print the sequence information
           into neat, aligned columns in the output file. Alternatively,
           leave it unset (N), in which case the information records will be
           delimited by a character, which you may specify by using the
           -delimiter option. In other words, if -columns is set on, the
           -delimiter option is overriden.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="string" name="delimiter">
      <default><![CDATA[|]]></default>
      <information><![CDATA[Delimiter of records in text output file]]></information>
      <help><![CDATA[This string, which is usually a single character only, is
           used to delimit individual records in the text output file. It
           could be a space character, a tab character, a pipe character or
           any other character or string.]]></help>
      <knowntype><![CDATA[output delimiter]]></knowntype>
      <relations><![CDATA[EDAM_data:2152 Rendering parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[stdout]]></default>
      <knowntype><![CDATA[infoseq output]]></knowntype>
      <help><![CDATA[If you enter the name of a file here then this program will
           write the sequence details into that file.]]></help>
      <relations><![CDATA[EDAM_data:0855 Sequence metadata]]></relations>
    </parameter>
    <parameter type="boolean" name="html">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Format output as an HTML table]]></information>
      <outputmodifier><![CDATA[Y]]></outputmodifier>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="only">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Display the specified columns]]></information>
      <help><![CDATA[This is a way of shortening the command line if you only
           want a few things to be displayed. Instead of specifying: \
           '-nohead -noname -noacc -notype -nopgc -nodesc' \
           to get only the length output, you can specify \
           '-only -length']]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="heading">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Display column headings]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="usa">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display the USA of the sequence]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="database">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'database' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="name">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'name' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="accession">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'accession' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="gi">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Display 'GI' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="seqversion">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Display 'version' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="type">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'type' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="length">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'length' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="pgc">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'percent GC content' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="organism">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'organism' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="description">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'description' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
</application>
