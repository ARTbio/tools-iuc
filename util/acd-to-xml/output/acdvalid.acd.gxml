<?xml version='1.0' encoding='UTF-8'?>
<tool id="acdvalid" name="acdvalid" version="6.6.0">
  <description>Validate an application ACD file</description>
  <macros>
    <token name="@EXECUTABLE@">acdvalid</token>
    <import>macros.xml</import>
  </macros>
  <expand macro="stdio"/>
  <expand macro="requirements"/>
  <command>acdvalid

#if $param_acdvalid_version:
  -acdvalid:version     "$param_acdvalid_version"
#end if
&gt; $param_stdout
</command>
  <inputs>
    <param name="param_acdvalid_version" type="text" size="30" value="6.6.0" label="Version of the tool that generated this parameters file" help="(-version) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
  </inputs>
  <outputs>
    <data name="param_stdout" format="text" label="Output from stdout"/>
  </outputs>
  <help>**What it does**

</help>
  <expand macro="references"/>
</tool>
