<?xml version="1.0"?>
<application id="prophet">
  <metadata>
    <documentation><![CDATA[Scan one or more sequences with a Gribskov
                  or Henikoff profile]]></documentation>
    <groups><![CDATA[Nucleic:Profiles, Protein:Profiles]]></groups>
    <gui><![CDATA[yes]]></gui>
    <batch><![CDATA[yes]]></batch>
    <cpu><![CDATA[medium]]></cpu>
    <relations><![CDATA[EDAM_topic:0188 Sequence profiles and HMMs]]></relations>
    <relations><![CDATA[EDAM_operation:0300 Sequence-profile alignment construction]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[any]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="infile" name="infile">
      <parameter><![CDATA[Y]]></parameter>
      <information><![CDATA[Profile or weight matrix file]]></information>
      <knowntype><![CDATA[profile or matrix]]></knowntype>
      <relations><![CDATA[EDAM_data:1354 Sequence profile]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="float" name="gapopen">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[1.0]]></default>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[100.0]]></maximum>
      <information><![CDATA[Gap opening coefficient]]></information>
      <relations><![CDATA[EDAM_data:1397 Gap opening penalty]]></relations>
    </parameter>
    <parameter type="float" name="gapextend">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[1.0]]></default>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[100.0]]></maximum>
      <information><![CDATA[Gap extension coefficient]]></information>
      <relations><![CDATA[EDAM_data:1398 Gap extension penalty]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="align" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <aformat><![CDATA[simple]]></aformat>
      <relations><![CDATA[EDAM_data:0863 Sequence alignment]]></relations>
    </parameter>
  </section>
</application>
