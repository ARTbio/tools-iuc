<?xml version="1.0"?>
<application id="redata">
  <metadata>
    <documentation><![CDATA[Retrieve information from REBASE restriction enzyme database]]></documentation>
    <groups><![CDATA[Nucleic:Restriction]]></groups>
    <relations><![CDATA[EDAM_topic:0100 Nucleic acid restriction]]></relations>
    <relations><![CDATA[EDAM_operation:2422 Data retrieval]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="string" name="enzyme">
      <parameter><![CDATA[Y]]></parameter>
      <information><![CDATA[Restriction enzyme name]]></information>
      <default><![CDATA[BamHI]]></default>
      <help><![CDATA[Enter the name of the restriction enzyme that you wish to
           get details of. The names often have a 'I' in them - this is a
           capital 'i', not a '1' or an 'l'. The names are case-independent
           ('AaeI' is the same as 'aaei')]]></help>
      <knowntype><![CDATA[enzyme name]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1013 Restriction enzyme name]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="isoschizomers">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Show isoschizomers]]></information>
      <help><![CDATA[Show other enzymes with this specificity. (Isoschizomers)]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="references">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Show references]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="suppliers">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Show suppliers]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[redata output]]></knowntype>
      <relations><![CDATA[EDAM_data:1517 Restriction enzyme property]]></relations>
    </parameter>
  </section>
</application>
