<?xml version="1.0"?>
<application id="listor">
  <metadata>
    <documentation><![CDATA[Write a list file of the logical OR of two
                  sets of sequences]]></documentation>
    <groups><![CDATA[Edit]]></groups>
    <relations><![CDATA[EDAM_topic:0091 Data handling]]></relations>
    <relations><![CDATA[EDAM_operation:2120 Listfile processing]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqset" name="firstsequences">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapany]]></type>
      <aligned><![CDATA[N]]></aligned>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="seqset" name="secondsequences">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapany]]></type>
      <aligned><![CDATA[N]]></aligned>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="list" name="operator">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[OR]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[1]]></maximum>
      <header><![CDATA[Logical operator to combine sequence lists]]></header>
      <values><![CDATA[OR:OR - merger of both sets, AND:AND - only those in both sets,
             XOR:XOR - only those not in both sets,
             NOT:NOT - those of the first set that are not in the second]]></values>
      <delimiter><![CDATA[,]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Enter the logical operator to combine the
                  sequences]]></information>
      <help><![CDATA[The following logical operators combine the sequences in
           the following ways: \
           OR - gives all that occur in one set or the other \
           AND - gives only those which occur in both sets \
           XOR - gives those which only occur in one set or the other, but
           not in both \
           NOT - gives those which occur in the first set except for those
           that also occur in the second]]></help>
      <relations><![CDATA[EDAM_identifier:2133 Logical operator]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <extension><![CDATA[list]]></extension>
      <knowntype><![CDATA[USA list]]></knowntype>
      <help><![CDATA[The list of sequence names will be written to this list
           file]]></help>
      <information><![CDATA[List of USAs output file]]></information>
      <relations><![CDATA[EDAM_data:1111 EMBOSS listfile]]></relations>
    </parameter>
  </section>
</application>
