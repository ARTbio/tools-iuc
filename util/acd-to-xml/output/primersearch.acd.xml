<?xml version="1.0"?>
<application id="primersearch">
  <metadata>
    <documentation><![CDATA[Search DNA sequences for matches with primer pairs]]></documentation>
    <groups><![CDATA[Nucleic:Primers]]></groups>
    <relations><![CDATA[EDAM_topic:0077 Nucleic acid analysis]]></relations>
    <relations><![CDATA[EDAM_topic:0090 Data search and retrieval]]></relations>
    <relations><![CDATA[EDAM_operation:0354 Sequence database search
               (by sequence for primer sequences)]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="seqall">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[dna]]></type>
      <relations><![CDATA[EDAM_data:2887 Sequence record (nucleic acid)]]></relations>
    </parameter>
    <parameter type="infile" name="infile">
      <parameter><![CDATA[Y]]></parameter>
      <information><![CDATA[Primer pairs file]]></information>
      <knowntype><![CDATA[primer pairs]]></knowntype>
      <relations><![CDATA[EDAM_format:1215 pure dna]]></relations>
      <relations><![CDATA[EDAM_data:0850 Sequence set]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="mismatchpercent">
      <parameter><![CDATA[Y]]></parameter>
      <default><![CDATA[0]]></default>
      <information><![CDATA[Allowed percent mismatch]]></information>
      <relations><![CDATA[EDAM_data:2146 Threshold]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[primer3 output]]></knowntype>
      <information><![CDATA[Whitehead primer3_core program output file]]></information>
      <relations><![CDATA[EDAM_data:0922 Nucleic acid features (primers)]]></relations>
    </parameter>
  </section>
</application>
