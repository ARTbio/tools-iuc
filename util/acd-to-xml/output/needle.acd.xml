<?xml version="1.0"?>
<application id="needle">
  <metadata>
    <documentation><![CDATA[Needleman-Wunsch global alignment of two sequences]]></documentation>
    <groups><![CDATA[Alignment:Global]]></groups>
    <gui><![CDATA[yes]]></gui>
    <batch><![CDATA[yes]]></batch>
    <cpu><![CDATA[medium]]></cpu>
    <relations><![CDATA[EDAM_topic:0182 Sequence alignment]]></relations>
    <relations><![CDATA[EDAM_operation:0494 Pairwise sequence alignment construction
    	       (global)]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="sequence" name="asequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[any]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="seqall" name="bsequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[@($(acdprotein) ? stopprotein : nucleotide)]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="matrixf" name="datafile">
      <additional><![CDATA[Y]]></additional>
      <information><![CDATA[Matrix file]]></information>
      <protein><![CDATA[$(acdprotein)]]></protein>
      <help><![CDATA[This is the scoring matrix file used when comparing
           sequences. By default it is the file 'EBLOSUM62' (for proteins) or
           the file 'EDNAFULL' (for nucleic sequences). These files are
           found in the 'data' directory of the EMBOSS installation.]]></help>
      <relations><![CDATA[EDAM_data:0874 Comparison matrix]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="float" name="gapopen">
      <standard><![CDATA[Y]]></standard>
      <information><![CDATA[Gap opening penalty]]></information>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[100.0]]></maximum>
      <default><![CDATA[@($(acdprotein)? 10.0 : 10.0 )]]></default>
      <valid><![CDATA[Floating point number from 1.0 to 100.0]]></valid>
      <expected><![CDATA[10.0 for any sequence]]></expected>
      <help><![CDATA[The gap open penalty is the score taken away when a gap is
           created. The best value depends on the choice of comparison
           matrix. The default value assumes you are using the EBLOSUM62
           matrix for protein sequences, and the EDNAFULL matrix for
           nucleotide sequences.]]></help>
      <relations><![CDATA[EDAM_data:1397 Gap opening penalty]]></relations>
    </parameter>
    <parameter type="float" name="gapextend">
      <standard><![CDATA[Y]]></standard>
      <information><![CDATA[Gap extension penalty]]></information>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[10.0]]></maximum>
      <default><![CDATA[@($(acdprotein)? 0.5 : 0.5 )]]></default>
      <valid><![CDATA[Floating point number from 0.0 to 10.0]]></valid>
      <expected><![CDATA[0.5 for any sequence]]></expected>
      <help><![CDATA[The gap extension, penalty is added to the standard gap
           penalty for each base or residue in the gap. This is how long gaps
           are penalized. Usually you will expect a few long gaps rather
           than many short gaps, so the gap extension penalty should be lower
           than the gap penalty. An exception is where one or both sequences
           are single reads with possible sequencing errors in which case
           you would expect many single base gaps. You can get this result by
           setting the gap open penalty to zero (or very low) and using the
           gap extension penalty to control gap scoring.]]></help>
      <relations><![CDATA[EDAM_data:1398 Gap extension penalty]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="endweight">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Apply end gap penalties.]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="float" name="endopen">
      <additional><![CDATA[Y]]></additional>
      <information><![CDATA[End gap opening penalty]]></information>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[100.0]]></maximum>
      <default><![CDATA[@($(acdprotein)? 10.0 : 10.0 )]]></default>
      <valid><![CDATA[Floating point number from 1.0 to 100.0]]></valid>
      <expected><![CDATA[10.0 for any sequence]]></expected>
      <help><![CDATA[The end gap open penalty is the score taken away when an end gap is
           created. The best value depends on the choice of comparison
           matrix. The default value assumes you are using the EBLOSUM62
           matrix for protein sequences, and the EDNAFULL matrix for
           nucleotide sequences.]]></help>
      <relations><![CDATA[EDAM_data:1410 Terminal gap opening penalty]]></relations>
    </parameter>
    <parameter type="float" name="endextend">
      <additional><![CDATA[Y]]></additional>
      <information><![CDATA[End gap extension penalty]]></information>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[10.0]]></maximum>
      <default><![CDATA[@($(acdprotein)? 0.5 : 0.5 )]]></default>
      <valid><![CDATA[Floating point number from 0.0 to 10.0]]></valid>
      <expected><![CDATA[0.5 for any sequence]]></expected>
      <help><![CDATA[The end gap extension, penalty is added to the end gap
           penalty for each base or residue in the end gap. This is how long end gaps
           are penalized.]]></help>
      <relations><![CDATA[EDAM_data:1411 Terminal gap extension penalty]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="brief">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Brief identity and similarity]]></information>
      <help><![CDATA[Brief identity and similarity]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="align" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <aformat><![CDATA[srspair]]></aformat>
      <minseqs><![CDATA[2]]></minseqs>
      <maxseqs><![CDATA[2]]></maxseqs>
      <aglobal><![CDATA[Y]]></aglobal>
      <relations><![CDATA[EDAM_data:1381 Sequence alignment (pair)]]></relations>
    </parameter>
  </section>
</application>
