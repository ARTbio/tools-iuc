<?xml version="1.0"?>
<application id="prophecy">
  <metadata>
    <documentation><![CDATA[Create frequency matrix or profile from a multiple alignment]]></documentation>
    <groups><![CDATA[Nucleic:Profiles, Protein:Profiles]]></groups>
    <relations><![CDATA[EDAM_topic:0188 Sequence profiles and HMMs]]></relations>
    <relations><![CDATA[EDAM_operation:0296 Sequence profile generation]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqset" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapany]]></type>
      <aligned><![CDATA[Y]]></aligned>
      <minseqs><![CDATA[2]]></minseqs>
      <relations><![CDATA[EDAM_data:0863 Sequence alignment]]></relations>
    </parameter>
    <parameter type="list" name="type">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[F]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[1]]></maximum>
      <values><![CDATA[F:Frequency,G:Gribskov,H:Henikoff]]></values>
      <delimiter><![CDATA[,]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <header><![CDATA[Profile type]]></header>
      <information><![CDATA[Select type]]></information>
      <button><![CDATA[Y]]></button>
      <relations><![CDATA[EDAM_data:2130 Sequence profile type]]></relations>
    </parameter>
    <parameter type="matrixf" name="datafile">
      <standard><![CDATA[@($(type) != F)]]></standard>
      <information><![CDATA[Scoring matrix]]></information>
      <default><![CDATA[@($(type) = G: Epprofile H: EBLOSUM62 F: EBLOSUM62)]]></default>
      <protein><![CDATA[$(acdprotein)]]></protein>
      <expected><![CDATA['Epprofile' for Gribskov type, or EBLOSUM62]]></expected>
      <relations><![CDATA[EDAM_data:0874 Comparison matrix]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="string" name="name">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[mymatrix]]></default>
      <information><![CDATA[Enter a name for the profile]]></information>
      <knowntype><![CDATA[name]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1115 Sequence profile ID]]></relations>
    </parameter>
    <section id="profiletypesection">
      <metadata>
        <information><![CDATA[Profile type specific options]]></information>
        <type><![CDATA[frame]]></type>
      </metadata>
      <parameter type="integer" name="threshold">
        <standard><![CDATA[@($(type) == F)]]></standard>
        <default><![CDATA[75]]></default>
        <minimum><![CDATA[1]]></minimum>
        <maximum><![CDATA[100]]></maximum>
        <information><![CDATA[Enter threshold reporting percentage]]></information>
        <relations><![CDATA[EDAM_data:2146 Threshold]]></relations>
      </parameter>
    </section>
    <section id="gapsection">
      <metadata>
        <information><![CDATA[Gap options]]></information>
        <type><![CDATA[frame]]></type>
      </metadata>
      <parameter type="float" name="open">
        <standard><![CDATA[@($(type) != F)]]></standard>
        <information><![CDATA[Gap opening penalty]]></information>
        <default><![CDATA[3.0]]></default>
        <relations><![CDATA[EDAM_data:1397 Gap opening penalty]]></relations>
      </parameter>
      <parameter type="float" name="extension">
        <standard><![CDATA[@($(type) != F)]]></standard>
        <information><![CDATA[Gap extension penalty]]></information>
        <default><![CDATA[0.3]]></default>
        <relations><![CDATA[EDAM_data:1398 Gap extension penalty]]></relations>
      </parameter>
    </section>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[prophecy output]]></knowntype>
      <relations><![CDATA[EDAM_data:1354 Sequence profile]]></relations>
    </parameter>
  </section>
</application>
