<?xml version="1.0"?>
<application id="checktrans">
  <metadata>
    <documentation><![CDATA[Report STOP codons and ORF statistics of a
                  protein]]></documentation>
    <groups><![CDATA[Nucleic:Gene finding, Nucleic:Translation]]></groups>
    <relations><![CDATA[EDAM_topic:0109 Gene finding]]></relations>
    <relations><![CDATA[EDAM_topic:0108 Translation]]></relations>
    <relations><![CDATA[EDAM_operation:0436 Coding region prediction]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[stopprotein]]></type>
      <relations><![CDATA[EDAM_data:2886 Sequence record (protein)]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="orfml">
      <standard><![CDATA[Y]]></standard>
      <minimum><![CDATA[1]]></minimum>
      <default><![CDATA[100]]></default>
      <information><![CDATA[Minimum ORF Length to report]]></information>
      <relations><![CDATA[EDAM_data:1249 Sequence length]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="addlast">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Force the sequence to end with an asterisk]]></information>
      <help><![CDATA[An asterisk in the protein sequence indicates the position
           of a STOP codon. Checktrans assumes that all ORFs end in a STOP
           codon. Forcing the sequence to end with an asterisk, if there is
           not one there already, makes checktrans treat the end as a
           potential ORF. If an asterisk is added, it is not included in the
           reported count of STOPs.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[checktrans output]]></knowntype>
      <relations><![CDATA[EDAM_data:1313 Nucleic acid features (coding sequence)]]></relations>
    </parameter>
    <parameter type="seqoutall" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <help><![CDATA[Sequence file to hold output ORF sequences]]></help>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="featout" name="outfeat">
      <parameter><![CDATA[Y]]></parameter>
      <help><![CDATA[File for output features]]></help>
      <type><![CDATA[protein]]></type>
      <relations><![CDATA[EDAM_data:1277 Protein features]]></relations>
    </parameter>
  </section>
</application>
