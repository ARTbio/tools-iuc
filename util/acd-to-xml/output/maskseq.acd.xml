<?xml version="1.0"?>
<application id="maskseq">
  <metadata>
    <documentation><![CDATA[Write a sequence with masked regions]]></documentation>
    <groups><![CDATA[Edit]]></groups>
    <relations><![CDATA[EDAM_topic:0091 Data handling]]></relations>
    <relations><![CDATA[EDAM_operation:0368 Sequence masking]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="sequence" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[any]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="range" name="regions">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[]]></default>
      <information><![CDATA[Regions to mask (eg: 4-57,78-94)]]></information>
      <help><![CDATA[Regions to mask. \
           A set of regions is specified by a set of pairs of positions. \
           The positions are integers. \
           They are separated by any non-digit, non-alpha character. \
           Examples of region specifications are: \
           24-45, 56-78 \
           1:45, 67=99;765..888 \
           1,5,8,10,23,45,57,99]]></help>
      <expected><![CDATA[None]]></expected>
      <relations><![CDATA[EDAM_data:1017 Sequence range]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="toggle" name="tolower">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Change masked region to lower-case]]></information>
      <help><![CDATA[The region can be 'masked' by converting the sequence
           characters to lower-case, some non-EMBOSS programs e.g. fasta can
           interpret this as a masked region. The sequence is unchanged apart
           from the case change. You might like to ensure that the whole
           sequence is in upper-case before masking the specified regions to
           lower-case by using the '-supper' flag.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="string" name="maskchar">
      <additional><![CDATA[@(!$(tolower))]]></additional>
      <default><![CDATA[@($(acdprotein)?X:N)]]></default>
      <information><![CDATA[Character to mask with]]></information>
      <help><![CDATA[Character to use when masking. \
           Default is 'X' for protein sequences, 'N' for nucleic sequences. \
           If the mask character is set to be the SPACE character or a null
           character, then the sequence is 'masked' by changing it to
           lower-case, just as with the '-lowercase' flag.]]></help>
      <expected><![CDATA['X' for protein, 'N' for nucleic]]></expected>
      <maxlength><![CDATA[1]]></maxlength>
      <minlength><![CDATA[1]]></minlength>
      <knowntype><![CDATA[sequence mask character]]></knowntype>
      <relations><![CDATA[EDAM_data:0851 Sequence mask character]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqout" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
</application>
