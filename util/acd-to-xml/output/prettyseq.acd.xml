<?xml version="1.0"?>
<application id="prettyseq">
  <metadata>
    <documentation><![CDATA[Write a nucleotide sequence and its translation to file]]></documentation>
    <groups><![CDATA[Nucleic:Translation, Display]]></groups>
    <relations><![CDATA[EDAM_topic:0108 Translation]]></relations>
    <relations><![CDATA[EDAM_topic:0092 Data visualisation]]></relations>
    <relations><![CDATA[EDAM_operation:0371 DNA translation]]></relations>
    <relations><![CDATA[EDAM_operation:0564 Sequence rendering]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="sequence" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[DNA]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="range" name="range">
      <standard><![CDATA[Y]]></standard>
      <information><![CDATA[Range(s) to translate]]></information>
      <default><![CDATA[$(sequence.begin)-$(sequence.end)]]></default>
      <expected><![CDATA[Whole sequence]]></expected>
      <relations><![CDATA[EDAM_data:1017 Sequence range]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="list" name="table">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[1]]></maximum>
      <header><![CDATA[Genetic codes]]></header>
      <values><![CDATA[0:Standard; 1:Standard (with alternative initiation
             codons); 2:Vertebrate Mitochondrial; 3:Yeast Mitochondrial;
             4:Mold, Protozoan, Coelenterate Mitochondrial and
             Mycoplasma/Spiroplasma; 5:Invertebrate Mitochondrial; 6:Ciliate
             Macronuclear and Dasycladacean; 9:Echinoderm Mitochondrial;
             10:Euplotid Nuclear; 11:Bacterial; 12:Alternative Yeast Nuclear;
             13:Ascidian Mitochondrial; 14:Flatworm Mitochondrial;
             15:Blepharisma Macronuclear; 16:Chlorophycean Mitochondrial;
             21:Trematode Mitochondrial; 22:Scenedesmus obliquus;
             23:Thraustochytrium Mitochondrial]]></values>
      <delimiter><![CDATA[;]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Genetic code to use]]></information>
      <relations><![CDATA[EDAM_identifier:2128 Genetic code name]]></relations>
    </parameter>
    <parameter type="boolean" name="ruler">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Add a ruler]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="plabel">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Number translations]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="nlabel">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Number DNA sequence]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="width">
      <minimum><![CDATA[10]]></minimum>
      <default><![CDATA[60]]></default>
      <information><![CDATA[Width of screen]]></information>
      <relations><![CDATA[EDAM_data:1249 Sequence length]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[prettyseq output]]></knowntype>
      <relations><![CDATA[EDAM_data:1313 Nucleic acid features (coding sequence)]]></relations>
    </parameter>
  </section>
</application>
