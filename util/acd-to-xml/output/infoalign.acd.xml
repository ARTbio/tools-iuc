<?xml version="1.0"?>
<application id="infoalign">
  <metadata>
    <documentation><![CDATA[Display basic information about a multiple sequence alignment]]></documentation>
    <groups><![CDATA[Alignment:Multiple, Data retrieval:Sequence data]]></groups>
    <relations><![CDATA[EDAM_topic:0182 Sequence alignment]]></relations>
    <relations><![CDATA[EDAM_topic:0090 Data search and retrieval]]></relations>
    <relations><![CDATA[EDAM_operation:2422 Data retrieval]]></relations>
    <relations><![CDATA[EDAM_operation:0564 Sequence rendering]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqset" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapany]]></type>
      <aligned><![CDATA[Y]]></aligned>
      <help><![CDATA[The sequence alignment to be displayed.]]></help>
      <relations><![CDATA[EDAM_data:0863 Sequence alignment]]></relations>
    </parameter>
    <parameter type="matrix" name="matrix">
      <additional><![CDATA[Y]]></additional>
      <information><![CDATA[Similarity scoring Matrix file]]></information>
      <protein><![CDATA[$(acdprotein)]]></protein>
      <help><![CDATA[This is the scoring matrix file used when comparing
           sequences. By default it is the file 'EBLOSUM62' (for proteins) or
           the file 'EDNAFULL' (for nucleic sequences). These files are
           found in the 'data' directory of the EMBOSS installation.]]></help>
      <relations><![CDATA[EDAM_data:0874 Comparison matrix]]></relations>
    </parameter>
    <parameter type="string" name="refseq">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <information><![CDATA[The number or the name of the reference
                  sequence]]></information>
      <help><![CDATA[If you give the number in the alignment or the name of a
           sequence, it will be taken to be the reference sequence. The
           reference sequence is the one against which all the other
           sequences are compared. If this is set to 0 then the consensus
           sequence will be used as the reference sequence. By default the
           consensus sequence is used as the reference sequence.]]></help>
      <knowntype><![CDATA[sequence id input]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1063 Sequence identifier]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="float" name="plurality">
      <default><![CDATA[50.0]]></default>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[100.0]]></maximum>
      <information><![CDATA[Plurality check % for consensus]]></information>
      <help><![CDATA[Set a cut-off for the % of positive scoring matches below
           which there is no consensus. The default plurality is taken as 50%
           of the total weight of all the sequences in the alignment.]]></help>
      <relations><![CDATA[EDAM_data:2146 Threshold]]></relations>
    </parameter>
    <parameter type="float" name="identity">
      <default><![CDATA[0.0]]></default>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[100.0]]></maximum>
      <information><![CDATA[Required % of identities at a position fro
                  consensus]]></information>
      <help><![CDATA[Provides the facility of setting the required number of
           identities at a position for it to give a consensus. Therefore, if
           this is set to 100% only columns of identities contribute to the
           consensus.]]></help>
      <relations><![CDATA[EDAM_data:2146 Threshold]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <extension><![CDATA[infoalign]]></extension>
      <knowntype><![CDATA[infoalign output]]></knowntype>
      <help><![CDATA[If you enter the name of a file here then this program will
           write the sequence details into that file.]]></help>
      <relations><![CDATA[EDAM_data:0866 Sequence alignment metadata]]></relations>
    </parameter>
    <parameter type="boolean" name="html">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Format output as an HTML table]]></information>
      <outputmodifier><![CDATA[Y]]></outputmodifier>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="only">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Display the specified columns]]></information>
      <help><![CDATA[This is a way of shortening the command line if you only
           want a few things to be displayed. Instead of specifying: \
           '-nohead -nousa -noname -noalign -nogaps -nogapcount -nosimcount
           -noidcount -nodiffcount -noweight' \
           to get only the sequence length output, you can specify \
           '-only -seqlength']]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="heading">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display column headings]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="usa">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display the USA of the sequence]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="name">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'name' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="seqlength">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'seqlength' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="alignlength">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'alignlength' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="gaps">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display number of gaps]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="gapcount">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display number of gap positions]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="idcount">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display number of identical positions]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="simcount">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display number of similar positions]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="diffcount">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display number of different positions]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="change">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display % number of changed positions]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="weight">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'weight' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="description">
      <default><![CDATA[@(!$(only))]]></default>
      <information><![CDATA[Display 'description' column]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
</application>
