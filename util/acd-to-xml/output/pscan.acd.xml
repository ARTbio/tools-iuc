<?xml version="1.0"?>
<application id="pscan">
  <metadata>
    <documentation><![CDATA[Scan protein sequence(s) with fingerprints from
                  the PRINTS database]]></documentation>
    <groups><![CDATA[Protein:Motifs]]></groups>
    <relations><![CDATA[EDAM_topic:0158 Sequence motifs]]></relations>
    <relations><![CDATA[EDAM_operation:3087 Protein site detection]]></relations>
    <relations><![CDATA[EDAM_operation:0347 Sequence database search
                (by motif or pattern)]]></relations>
    <relations><![CDATA[EDAM_operation:0340 Protein secondary database search]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[Protein]]></type>
      <relations><![CDATA[EDAM_data:2886 Sequence record (protein)]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="emin">
      <standard><![CDATA[Y]]></standard>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[20]]></maximum>
      <default><![CDATA[2]]></default>
      <information><![CDATA[Minimum number of elements per fingerprint]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="integer" name="emax">
      <standard><![CDATA[Y]]></standard>
      <minimum><![CDATA[$(emin)]]></minimum>
      <maximum><![CDATA[20]]></maximum>
      <failrange><![CDATA[N]]></failrange>
      <trueminimum><![CDATA[N]]></trueminimum>
      <default><![CDATA[20]]></default>
      <information><![CDATA[Maximum number of elements per fingerprint]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[pscan output]]></knowntype>
      <relations><![CDATA[EDAM_data:1420 Sequence-profile alignment (fingerprint)]]></relations>
    </parameter>
  </section>
</application>
