<?xml version="1.0"?>
<application id="emowse">
  <metadata>
    <documentation><![CDATA[Search protein sequences by digest fragment molecular weight]]></documentation>
    <groups><![CDATA[Protein:Composition]]></groups>
    <relations><![CDATA[EDAM_topic:0157 Sequence composition analysis]]></relations>
    <relations><![CDATA[EDAM_topic:0767 Protein and peptide identification]]></relations>
    <relations><![CDATA[EDAM_operation:0355 Sequence database search
              (by molecular weight)]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[protein]]></type>
      <relations><![CDATA[EDAM_data:2886 Sequence record (protein)]]></relations>
    </parameter>
    <parameter type="infile" name="infile">
      <parameter><![CDATA[Y]]></parameter>
      <information><![CDATA[Peptide molecular weight values file]]></information>
      <knowntype><![CDATA[peptide molweights]]></knowntype>
      <relations><![CDATA[EDAM_data:1519 Peptide molecular weights]]></relations>
    </parameter>
    <parameter type="datafile" name="mwdata">
      <information><![CDATA[Molecular weights data file]]></information>
      <default><![CDATA[Emolwt.dat]]></default>
      <knowntype><![CDATA[molecular weights]]></knowntype>
      <relations><![CDATA[EDAM_data:1505 Amino acid index (molecular weight)]]></relations>
    </parameter>
    <parameter type="datafile" name="frequencies">
      <default><![CDATA[Efreqs.dat]]></default>
      <information><![CDATA[Amino acid frequencies data file]]></information>
      <knowntype><![CDATA[amino acid frequencies]]></knowntype>
      <relations><![CDATA[EDAM_data:1519 Peptide molecular weights]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="weight">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[0]]></default>
      <information><![CDATA[Whole sequence molwt]]></information>
      <relations><![CDATA[EDAM_data:0844 Molecular mass]]></relations>
    </parameter>
    <parameter type="boolean" name="mono">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Use monoisotopic weights]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="list" name="enzyme">
      <default><![CDATA[1]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[1]]></maximum>
      <header><![CDATA[Enzymes and reagents]]></header>
      <values><![CDATA[1:Trypsin, 2:Lys-C, 3:Arg-C, 4:Asp-N, 5:V8-bicarb,
             6:V8-phosph, 7:Chymotrypsin, 8:CNBr]]></values>
      <delimiter><![CDATA[,]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Enzyme or reagent]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="integer" name="pcrange">
      <default><![CDATA[25]]></default>
      <minimum><![CDATA[0]]></minimum>
      <maximum><![CDATA[75]]></maximum>
      <information><![CDATA[Allowed whole sequence weight variability]]></information>
      <relations><![CDATA[EDAM_data:0844 Molecular mass]]></relations>
    </parameter>
    <parameter type="float" name="tolerance">
      <default><![CDATA[0.1]]></default>
      <minimum><![CDATA[0.1]]></minimum>
      <maximum><![CDATA[1.0]]></maximum>
      <information><![CDATA[Tolerance]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="float" name="partials">
      <default><![CDATA[0.4]]></default>
      <minimum><![CDATA[0.1]]></minimum>
      <maximum><![CDATA[1.0]]></maximum>
      <information><![CDATA[Partials factor]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[emowse output]]></knowntype>
      <relations><![CDATA[EDAM_data:1262 Peptide molecular weight hits]]></relations>
    </parameter>
  </section>
</application>
