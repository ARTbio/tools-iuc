<?xml version="1.0"?>
<application id="psiphi">
  <metadata>
    <documentation><![CDATA[Calculates phi and psi torsion angles from
                  protein coordinates]]></documentation>
    <groups><![CDATA[protein:3D structure]]></groups>
    <relations><![CDATA[EDAM_topic:2814 Protein structure analysis]]></relations>
    <relations><![CDATA[EDAM_operation:0249 Torsion angle calculation]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="infile" name="infile">
      <parameter><![CDATA[Y]]></parameter>
      <information><![CDATA[Clean protein structure coordinates file]]></information>
      <knowntype><![CDATA[protein clean coordinates]]></knowntype>
      <relations><![CDATA[EDAM_data:1460 Protein structure]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="chainnumber">
      <standard><![CDATA[Y]]></standard>
      <information><![CDATA[Number of the chain for which torsion angles
                  should be calculated]]></information>
      <default><![CDATA[1]]></default>
      <minimum><![CDATA[1]]></minimum>
      <relations><![CDATA[EDAM_identifier:1008 Polypeptide chain ID]]></relations>
    </parameter>
    <parameter type="integer" name="startresiduenumber">
      <standard><![CDATA[Y]]></standard>
      <information><![CDATA[First residue in chain for which torsion
                  angles should be calculated]]></information>
      <default><![CDATA[1]]></default>
      <minimum><![CDATA[1]]></minimum>
      <relations><![CDATA[EDAM_data:1016 Sequence position]]></relations>
    </parameter>
    <parameter type="integer" name="finishresiduenumber">
      <standard><![CDATA[Y]]></standard>
      <information><![CDATA[Last residue in chain for which torsion
                  angles should be calculated (1 = last residue)]]></information>
      <default><![CDATA[1]]></default>
      <relations><![CDATA[EDAM_data:1016 Sequence position]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="report" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <rformat><![CDATA[table]]></rformat>
      <multiple><![CDATA[N]]></multiple>
      <precision><![CDATA[1]]></precision>
      <taglist><![CDATA[float:phi float:psi]]></taglist>
      <relations><![CDATA[EDAM_data:1544 Ramachandran plot]]></relations>
    </parameter>
  </section>
</application>
