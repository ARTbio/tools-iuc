<?xml version='1.0' encoding='UTF-8'?>
<tool id="dbxresource" name="dbxresource" version="6.6.0">
  <description>Index a data resource catalogue using b+tree indices</description>
  <macros>
    <token name="@EXECUTABLE@">dbxresource</token>
    <import>macros.xml</import>
  </macros>
  <expand macro="stdio"/>
  <expand macro="requirements"/>
  <command>dbxresource

#if $param_dbxresource_version:
  -dbxresource:version     "$param_dbxresource_version"
#end if
#if $param_input_dbname:
  -input:dbname     "$param_input_dbname"
#end if
#if $param_input_standardname:
  -input:standardname     "$param_input_standardname"
#end if
#if $param_input_dbresource:
  -input:dbresource     "$param_input_dbresource"
#end if
#if $param_input_directory:
  -input:directory $param_input_directory
#end if
#if $param_input_filenames:
  -input:filenames     "$param_input_filenames"
#end if
#if $param_required_fields:
  -required:fields
  #if " " in str($param_required_fields):
    "$param_required_fields"
  #else
    $param_required_fields
  #end if
#end if
#if $param_required_compressed:
  -required:compressed
#end if
#if $param_advanced_release:
  -advanced:release     "$param_advanced_release"
#end if
#if $param_advanced_date:
  -advanced:date     "$param_advanced_date"
#end if
#if $param_advanced_exclude:
  -advanced:exclude     "$param_advanced_exclude"
#end if
#if $param_indexoutdir:
  -indexoutdir $param_indexoutdir
#end if
#if $param_outfile:
  -outfile $param_outfile
#end if
</command>
  <inputs>
    <param name="param_dbxresource_version" type="text" size="30" value="6.6.0" label="Version of the tool that generated this parameters file" help="(-version) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
    <param name="param_input_dbname" type="text" size="30" label="Basename for index files" help="(-dbname) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
    <param name="param_input_standardname" type="text" size="30" label="Standard database name" help="(-standardname) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
    <param name="param_input_dbresource" type="text" size="30" label="Resource name" help="(-dbresource) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
    <param name="param_input_directory" type="data" format="" optional="True" label="Database directory" help="(-directory) "/>
    <param name="param_input_filenames" type="text" size="30" label="Wildcard database filename" help="(-filenames) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
    <param name="param_required_fields" type="select" optional="True" label="Index fields" help="(-fields) ">
      <option value="IDother">IDother</option>
      <option value="Link">Link</option>
      <option value="EDAM topic term">EDAM topic term</option>
      <option value="Query output format">Query output format</option>
      <option value="Server URL">Server URL</option>
      <option value="SOAP URL">SOAP URL</option>
      <option value="Description">Description</option>
      <option value="Taxon id">Taxon id</option>
      <option value="Rest URL">Rest URL</option>
      <option value="Category name">Category name</option>
      <option value="EDAM data term">EDAM data term</option>
      <option value="Query URL">Query URL</option>
      <option value="EDAM format term">EDAM format term</option>
      <option value="Query output">Query output</option>
      <option value="EDAM data id term">EDAM data id term</option>
      <option value="Name">Name</option>
      <option value="Query input parameters">Query input parameters</option>
      <option value="ID">ID</option>
    </param>
    <param name="param_required_compressed" type="boolean" truevalue="-required:compressed" falsevalue="" optional="True" label="Compressed index files" help="(-compressed) "/>
    <param name="param_advanced_release" type="text" size="30" label="Release numbe" help="(-release) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
    <param name="param_advanced_date" type="text" size="30" label="Index date" help="(-date) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
    <param name="param_advanced_exclude" type="text" size="30" label="Wildcard filename(s) to exclude" help="(-exclude) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
  </inputs>
  <outputs>
    <data name="param_indexoutdir" format="data"/>
    <data name="param_outfile" format="data"/>
  </outputs>
  <help>**What it does**

</help>
  <expand macro="references"/>
</tool>
