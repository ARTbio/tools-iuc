<?xml version="1.0"?>
<application id="msbar">
  <metadata>
    <documentation><![CDATA[Mutate a sequence]]></documentation>
    <groups><![CDATA[Nucleic:Mutation, Protein:Mutation]]></groups>
    <relations><![CDATA[EDAM_topic:0091 Data handling]]></relations>
    <relations><![CDATA[EDAM_topic:0199 Genetic variation]]></relations>
    <relations><![CDATA[EDAM_operation:0367 Sequence mutation and randomization]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[any]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="seqall" name="othersequence">
      <type><![CDATA[any]]></type>
      <default><![CDATA[asis:N]]></default>
      <information><![CDATA[Other sequences that the mutated result
                  should not match]]></information>
      <help><![CDATA[If you require that the resulting mutated sequence should
           not match a set of other sequences, then you can specify that set
           of sequences here. For example, if you require that the mutated
           sequence should not be the same as the input sequence, enter the
           input sequence here. If you want the result to be different to
           previous results of this program, specify the previous result
           sequences here. The program will check that the result does not
           match the sequences specified here before writing it out. If a
           match is found, then the mutation is started again with a fresh
           copy of the input sequence. If, after 10 such retries, there is
           still a match to the set of sequence given here, then the matching
           mutated sequence is written with a warning message.]]></help>
      <nullok><![CDATA[Y]]></nullok>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="count">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[1]]></default>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Number of times to perform the mutation
                  operations]]></information>
      <relations><![CDATA[EDAM_data:1693 Number of iterations]]></relations>
    </parameter>
    <parameter type="list" name="point">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[4]]></maximum>
      <header><![CDATA[Point mutation operations]]></header>
      <values><![CDATA[0:None; 1:Any of the following; 2:Insertions;
             3:Deletions; 4:Changes; 5:Duplications; 6:Moves]]></values>
      <delimiter><![CDATA[;]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Types of point mutations to perform]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="list" name="block">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[4]]></maximum>
      <header><![CDATA[Block mutation operations]]></header>
      <values><![CDATA[0:None; 1:Any of the following; 2:Insertions;
             3:Deletions; 4:Changes; 5:Duplications; 6:Moves]]></values>
      <delimiter><![CDATA[;]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Types of block mutations to perform]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="list" name="codon">
      <standard><![CDATA[@(!$(acdprotein))]]></standard>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[4]]></maximum>
      <header><![CDATA[Codon mutation operations]]></header>
      <values><![CDATA[0:None; 1:Any of the following; 2:Insertions;
             3:Deletions; 4:Changes; 5:Duplications; 6:Moves]]></values>
      <delimiter><![CDATA[;]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Types of codon mutations to perform]]></information>
      <help><![CDATA[Types of codon mutations to perform. These are only done if
           the sequence is nucleic.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="inframe">
      <additional><![CDATA[@(!$(acdprotein))]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Do 'codon' and 'block' operations in frame]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="minimum">
      <default><![CDATA[1]]></default>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Minimum size for a block mutation]]></information>
      <relations><![CDATA[EDAM_data:1249 Sequence length]]></relations>
    </parameter>
    <parameter type="integer" name="maximum">
      <default><![CDATA[10]]></default>
      <minimum><![CDATA[$(minimum)]]></minimum>
      <information><![CDATA[Maximum size for a block mutation]]></information>
      <relations><![CDATA[EDAM_data:1249 Sequence length]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqoutall" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
</application>
