<?xml version="1.0"?>
<application id="tranalign">
  <metadata>
    <documentation><![CDATA[Generate an alignment of nucleic coding regions
                  from aligned proteins]]></documentation>
    <groups><![CDATA[Alignment:Multiple]]></groups>
    <relations><![CDATA[EDAM_topic:0182 Sequence alignment]]></relations>
    <relations><![CDATA[EDAM_operation:0260 Sequence alignment conversion]]></relations>
    <relations><![CDATA[EDAM_operation:0371 DNA translation]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="asequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[nucleotide]]></type>
      <relations><![CDATA[EDAM_format:1210 pure nucleotide]]></relations>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="seqset" name="bsequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapprotein]]></type>
      <aligned><![CDATA[Y]]></aligned>
      <relations><![CDATA[EDAM_data:1384 Sequence alignment (protein)]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="list" name="table">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[1]]></maximum>
      <header><![CDATA[Genetic codes]]></header>
      <values><![CDATA[0:Standard; 1:Standard (with alternative initiation
             codons); 2:Vertebrate Mitochondrial; 3:Yeast Mitochondrial;
             4:Mold, Protozoan, Coelenterate Mitochondrial and
             Mycoplasma/Spiroplasma; 5:Invertebrate Mitochondrial; 6:Ciliate
             Macronuclear and Dasycladacean; 9:Echinoderm Mitochondrial;
             10:Euplotid Nuclear; 11:Bacterial; 12:Alternative Yeast Nuclear;
             13:Ascidian Mitochondrial; 14:Flatworm Mitochondrial;
             15:Blepharisma Macronuclear; 16:Chlorophycean Mitochondrial;
             21:Trematode Mitochondrial; 22:Scenedesmus obliquus;
             23:Thraustochytrium Mitochondrial]]></values>
      <delimiter><![CDATA[;]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Code to use]]></information>
      <relations><![CDATA[EDAM_identifier:2128 Genetic code name]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqoutset" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapnucleotide]]></type>
      <aligned><![CDATA[Y]]></aligned>
      <relations><![CDATA[EDAM_data:2887 Sequence record (nucleic acid)]]></relations>
    </parameter>
  </section>
</application>
