<?xml version="1.0"?>
<application id="abiview">
  <metadata>
    <documentation><![CDATA[Display the trace in an ABI sequencer file]]></documentation>
    <groups><![CDATA[Data retrieval:Sequence data, Display]]></groups>
    <relations><![CDATA[EDAM_topic:0092 Data visualisation]]></relations>
    <relations><![CDATA[EDAM_topic:0090 Data search and retrieval]]></relations>
    <relations><![CDATA[EDAM_operation:0337 Plotting and rendering]]></relations>
    <relations><![CDATA[EDAM_operation:0564 Sequence rendering]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="infile" name="infile">
      <parameter><![CDATA[Y]]></parameter>
      <default><![CDATA[]]></default>
      <binary><![CDATA[Y]]></binary>
      <knowntype><![CDATA[abi trace]]></knowntype>
      <information><![CDATA[ABI sequencing trace file]]></information>
      <relations><![CDATA[EDAM_data:0924 Sequence trace]]></relations>
      <relations><![CDATA[EDAM_format:1207 nucleotide]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqout" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[dna]]></type>
      <relations><![CDATA[EDAM_data:2887 Sequence record (nucleic acid)]]></relations>
    </parameter>
    <parameter type="xygraph" name="graph">
      <standard><![CDATA[Y]]></standard>
      <gxtitle><![CDATA[Residue Position]]></gxtitle>
      <relations><![CDATA[EDAM_data:2168 Sequence trace image]]></relations>
      <sequence><![CDATA[Y]]></sequence>
    </parameter>
    <parameter type="integer" name="startbase">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[First base to report or display]]></information>
      <relations><![CDATA[EDAM_data:1016 Sequence position]]></relations>
    </parameter>
    <parameter type="integer" name="endbase">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <information><![CDATA[Last base to report or display]]></information>
      <help><![CDATA[Last sequence base to report or display. If the default is
           set to zero then the value of this is taken as the maximum number
           of bases.]]></help>
      <relations><![CDATA[EDAM_data:1016 Sequence position]]></relations>
    </parameter>
    <parameter type="boolean" name="separate">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Separate the trace graphs for the 4 bases]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="yticks">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Display y-axis ticks]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="sequence">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Display the sequence on the graph]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="integer" name="window">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[40]]></default>
      <information><![CDATA[Sequence display window size]]></information>
      <relations><![CDATA[EDAM_data:1251 Window size]]></relations>
    </parameter>
    <parameter type="string" name="bases">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[GATC]]></default>
      <information><![CDATA[Base graphs to be displayed]]></information>
      <pattern><![CDATA[[GATC]+]]></pattern>
      <knowntype><![CDATA[nucleotide codes]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1007 Nucleotide code]]></relations>
    </parameter>
  </section>
</application>
