<?xml version="1.0"?>
<application id="pepcoil">
  <metadata>
    <documentation><![CDATA[Predict coiled coil regions in protein sequences]]></documentation>
    <groups><![CDATA[Protein:2D Structure, Protein:Domains]]></groups>
    <relations><![CDATA[EDAM_topic:0178 Protein secondary structure prediction]]></relations>
    <relations><![CDATA[EDAM_topic:0736 Protein domains and folds]]></relations>
    <relations><![CDATA[EDAM_operation:0268 Protein super-secondary structure prediction]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[PureProtein]]></type>
      <relations><![CDATA[EDAM_data:2886 Sequence record (protein)]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="window">
      <standard><![CDATA[Y]]></standard>
      <minimum><![CDATA[7]]></minimum>
      <maximum><![CDATA[28]]></maximum>
      <default><![CDATA[28]]></default>
      <information><![CDATA[Window size]]></information>
      <relations><![CDATA[EDAM_data:1251 Window size]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="coil">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Report coiled coil regions]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="frame">
      <default><![CDATA[$(coil)]]></default>
      <information><![CDATA[Show coil frameshifts]]></information>
      <expected><![CDATA[Yes if -coil is true]]></expected>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="other">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Report non coiled coil regions]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="report" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <rformat><![CDATA[motif]]></rformat>
      <taglist><![CDATA[float:Probability str:Predict int:pos=Max_coil_pos str:Frames]]></taglist>
      <relations><![CDATA[EDAM_data:0877 Protein features (super-secondary)]]></relations>
