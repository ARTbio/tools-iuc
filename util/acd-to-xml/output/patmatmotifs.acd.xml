<?xml version="1.0"?>
<application id="patmatmotifs">
  <metadata>
    <documentation><![CDATA[Scan a protein sequence with motifs from the PROSITE database]]></documentation>
    <groups><![CDATA[Protein:Motifs]]></groups>
    <relations><![CDATA[EDAM_topic:0158 Sequence motifs]]></relations>
    <relations><![CDATA[EDAM_operation:3087 Protein site detection]]></relations>
    <relations><![CDATA[EDAM_operation:0340 Protein secondary database search]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="sequence" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[Protein]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="full">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Provide full documentation for matching
                  patterns]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="prune">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Ignore simple patterns]]></information>
      <help><![CDATA[Ignore simple patterns. If this is true then these simple
           post-translational modification sites are not reported: myristyl,
