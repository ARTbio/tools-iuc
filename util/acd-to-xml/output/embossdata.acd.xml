<?xml version="1.0"?>
<application id="embossdata">
  <metadata>
    <documentation><![CDATA[Find and retrieve EMBOSS data files]]></documentation>
    <groups><![CDATA[Utils]]></groups>
    <relations><![CDATA[EDAM_topic:0091 Data handling]]></relations>
    <relations><![CDATA[EDAM_operation:2422 Data retrieval]]></relations>
    <relations><![CDATA[EDAM_operation:1812 Data loading]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="toggle" name="showall">
      <additional><![CDATA[Y]]></additional>
      <information><![CDATA[Show all potential EMBOSS data files]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="fetch">
      <additional><![CDATA[@(!$(showall))]]></additional>
      <information><![CDATA[Fetch a data file]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="string" name="filename">
      <parameter><![CDATA[Y]]></parameter>
      <default><![CDATA[]]></default>
      <information><![CDATA[Data file name]]></information>
      <help><![CDATA[This specifies the name of the file that should be fetched
           into the current directory or searched for in all of the
           directories that EMBOSS programs search when looking for a data
           file. The name of the file is not altered when it is fetched.]]></help>
      <knowntype><![CDATA[filename]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1050 File name]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="selection" name="reject">
      <default><![CDATA[3, 5, 6]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[6]]></maximum>
      <values><![CDATA[None, AAINDEX, CVS, CODONS, PRINTS, PROSITE, REBASE]]></values>
      <delimiter><![CDATA[,]]></delimiter>
      <header><![CDATA[Directories to ignore]]></header>
      <information><![CDATA[Select directories]]></information>
      <help><![CDATA[This specifies the names of the sub-directories of the
           EMBOSS data directory that should be ignored when displaying data
           directories.]]></help>
      <button><![CDATA[Y]]></button>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[stdout]]></default>
      <help><![CDATA[This specifies the name of the file that the results of a
           search for a file in the various data directories is written to.
           By default these results are written to the screen (stdout).]]></help>
      <knowntype><![CDATA[emboss datafile]]></knowntype>
      <information><![CDATA[EMBOSS datafile output file]]></information>
      <relations><![CDATA[EDAM_data:0957 Database metadata]]></relations>
    </parameter>
  </section>
</application>
