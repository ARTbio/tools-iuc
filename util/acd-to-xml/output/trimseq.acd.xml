<?xml version="1.0"?>
<application id="trimseq">
  <metadata>
    <documentation><![CDATA[Remove unwanted characters from start and end of sequence(s)]]></documentation>
    <groups><![CDATA[Edit]]></groups>
    <relations><![CDATA[EDAM_topic:0091 Data handling]]></relations>
    <relations><![CDATA[EDAM_operation:0369 Sequence cutting]]></relations>
    <relations><![CDATA[EDAM_operation:0235 Sequence ambiguity calculation]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapany]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="window">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[1]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[$(sequence.length)]]></maximum>
      <failrange><![CDATA[N]]></failrange>
      <trueminimum><![CDATA[Y]]></trueminimum>
      <information><![CDATA[Window size]]></information>
      <help><![CDATA[This determines the size of the region that is considered
           when deciding whether the percentage of ambiguity is greater than
           the threshold. A value of 5 means that a region of 5 letters in
           the sequence is shifted along the sequence from the ends and
           trimming is done only if there is a greater or equal percentage of
           ambiguity than the threshold percentage.]]></help>
      <relations><![CDATA[EDAM_data:1251 Window size]]></relations>
    </parameter>
    <parameter type="float" name="percent">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[100.0]]></default>
      <information><![CDATA[Percent threshold of ambiguity in window]]></information>
      <help><![CDATA[This is the threshold of the percentage ambiguity in the
           window required in order to trim a sequence.]]></help>
      <relations><![CDATA[EDAM_data:2146 Threshold]]></relations>
    </parameter>
    <parameter type="boolean" name="strict">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Trim off all ambiguity codes, not just N or
                  X]]></information>
      <help><![CDATA[In nucleic sequences, trim off not only N's and X's, but
           also the nucleotide IUPAC ambiguity codes M, R, W, S, Y, K, V, H,
           D and B. In protein sequences, trim off not only X's but also B
           and Z.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="star">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Trim off asterisks]]></information>
      <help><![CDATA[In protein sequences, trim off not only X's, but also the
           *'s]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="left">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Trim at the start]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="right">
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Trim at the end]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqoutall" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
</application>
