<?xml version="1.0"?>
<application id="marscan">
  <metadata>
    <documentation><![CDATA[Find matrix/scaffold recognition (MRS) signatures
                  in DNA sequences]]></documentation>
    <groups><![CDATA[Nucleic:Gene finding, Nucleic:Functional sites, Nucleic:Transcription]]></groups>
    <relations><![CDATA[EDAM_topic:0109 Gene finding]]></relations>
    <relations><![CDATA[EDAM_topic:0747 Nucleic acid sites and features]]></relations>
    <relations><![CDATA[EDAM_topic:0110 Transcription]]></relations>
    <relations><![CDATA[EDAM_operation:0444 Matrix/scaffold attachment site prediction]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[dna]]></type>
      <relations><![CDATA[EDAM_data:2887 Sequence record (nucleic acid)]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="report" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <taglist><![CDATA[str:type int:start8bp int:end8bp int:start16bp
              int:end16bp]]></taglist>
      <rformat><![CDATA[gff]]></rformat>
      <help><![CDATA[File for output of MAR/SAR recognition signature (MRS)
           regions. This contains details of the MRS in normal GFF format.
           The MRS consists of two recognition sites, one of 8 bp and one of
           16 bp on either sense strand of the genomic DNA, within 200 bp of
           each other.]]></help>
      <relations><![CDATA[EDAM_data:1276 Nucleic acid features]]></relations>
    </parameter>
  </section>
</application>
