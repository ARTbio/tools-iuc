<?xml version="1.0"?>
<application id="einverted">
  <metadata>
    <documentation><![CDATA[Find inverted repeats in nucleotide sequences]]></documentation>
    <groups><![CDATA[Nucleic:Repeats, Nucleic:2D structure]]></groups>
    <gui><![CDATA[yes]]></gui>
    <batch><![CDATA[yes]]></batch>
    <cpu><![CDATA[medium]]></cpu>
    <relations><![CDATA[EDAM_topic:0157 Sequence composition analysis]]></relations>
    <relations><![CDATA[EDAM_topic:0097 Nucleic acid structure analysis]]></relations>
    <relations><![CDATA[EDAM_operation:0379 Repeat sequence detection]]></relations>
    <relations><![CDATA[EDAM_operation:0278 RNA secondary structure prediction]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[dna]]></type>
      <relations><![CDATA[EDAM_data:2887 Sequence record (nucleic acid)]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="gap">
      <default><![CDATA[12]]></default>
      <standard><![CDATA[Y]]></standard>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Gap penalty]]></information>
      <relations><![CDATA[EDAM_data:2137 Gap penalty]]></relations>
    </parameter>
    <parameter type="integer" name="threshold">
      <default><![CDATA[50]]></default>
      <standard><![CDATA[Y]]></standard>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Minimum score threshold]]></information>
      <relations><![CDATA[EDAM_data:2146 Threshold]]></relations>
    </parameter>
    <parameter type="integer" name="match">
      <default><![CDATA[3]]></default>
      <standard><![CDATA[Y]]></standard>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Match score]]></information>
      <relations><![CDATA[EDAM_data:1401 Match reward score]]></relations>
    </parameter>
    <parameter type="integer" name="mismatch">
      <default><![CDATA[-4]]></default>
      <standard><![CDATA[Y]]></standard>
      <maximum><![CDATA[0]]></maximum>
      <information><![CDATA[Mismatch score]]></information>
      <relations><![CDATA[EDAM_data:1402 Mismatch penalty score]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="maxrepeat">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[2000]]></default>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Maximum extent of repeats]]></information>
      <help><![CDATA[Maximum separation between the start of repeat and the end
           of the inverted repeat.]]></help>
      <relations><![CDATA[EDAM_data:1249 Sequence length]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <extension><![CDATA[inv]]></extension>
      <knowntype><![CDATA[inverted output]]></knowntype>
      <information><![CDATA[Sanger Centre program inverted output file]]></information>
      <relations><![CDATA[EDAM_data:2048 Report]]></relations>
    </parameter>
    <parameter type="seqout" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <nullok><![CDATA[Y]]></nullok>
      <information><![CDATA[File for sequence of regions of inverted
                  repeats.]]></information>
      <help><![CDATA[The sequence of the inverted repeat regions without gap
           characters.]]></help>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
</application>
