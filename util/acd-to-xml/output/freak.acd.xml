<?xml version="1.0"?>
<application id="freak">
  <metadata>
    <documentation><![CDATA[Generate residue/base frequency table or plot]]></documentation>
    <groups><![CDATA[Nucleic:Composition, Protein:Composition]]></groups>
    <relations><![CDATA[EDAM_topic:0157 Sequence composition analysis]]></relations>
    <relations><![CDATA[EDAM_operation:0236 Sequence composition calculation]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="seqall">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[any]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="string" name="letters">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[gc]]></default>
      <information><![CDATA[Residue letters]]></information>
      <knowntype><![CDATA[nucleotide sequence]]></knowntype>
      <relations><![CDATA[EDAM_format:1210 pure nucleotide]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="step">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[1]]></default>
      <information><![CDATA[Stepping value]]></information>
      <relations><![CDATA[EDAM_data:2141 Window step size]]></relations>
    </parameter>
    <parameter type="integer" name="window">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[30]]></default>
      <information><![CDATA[Averaging window]]></information>
      <relations><![CDATA[EDAM_data:1251 Window size]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="toggle" name="plot">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Produce graphic]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="xygraph" name="graph">
      <standard><![CDATA[$(plot)]]></standard>
      <relations><![CDATA[EDAM_data:2166 Sequence composition plot]]></relations>
      <sequence><![CDATA[Y]]></sequence>
    </parameter>
    <parameter type="outfile" name="outfile">
      <standard><![CDATA[@(!$(plot))]]></standard>
      <knowntype><![CDATA[freak output]]></knowntype>
      <nullok><![CDATA[$(plot)]]></nullok>
      <nulldefault><![CDATA[$(plot)]]></nulldefault>
      <relations><![CDATA[EDAM_data:1261 Sequence composition]]></relations>
    </parameter>
  </section>
</application>
