<?xml version="1.0"?>
<application id="banana">
  <metadata>
    <documentation><![CDATA[Plot bending and curvature data for B-DNA]]></documentation>
    <groups><![CDATA[Nucleic:Properties, Nucleic:Composition, Nucleic:2D structure]]></groups>
    <relations><![CDATA[EDAM_topic:0097 Nucleic acid structure analysis]]></relations>
    <relations><![CDATA[EDAM_topic:0094 Nucleic acid thermodynamics]]></relations>
    <relations><![CDATA[EDAM_topic:0157 Sequence composition analysis]]></relations>
    <relations><![CDATA[EDAM_operation:0461 Nucleic acid curvature calculation]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="sequence" name="sequence">
      <parameter><![CDATA[y]]></parameter>
      <type><![CDATA[puredna]]></type>
      <relations><![CDATA[EDAM_format:1214 unambiguous pure dna]]></relations>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="datafile" name="anglesfile">
      <additional><![CDATA[Y]]></additional>
      <information><![CDATA[DNA base trimer roll angles data file]]></information>
      <default><![CDATA[Eangles_tri.dat]]></default>
      <knowntype><![CDATA[base trimer roll angles]]></knowntype>
      <relations><![CDATA[EDAM_data:1590 DNA base trimer roll angles data]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="graph" name="graph">
      <standard><![CDATA[Y]]></standard>
      <gdesc><![CDATA[Bending and curvature plot]]></gdesc>
      <nullok><![CDATA[Y]]></nullok>
      <relations><![CDATA[EDAM_data:0912 Nucleic acid property]]></relations>
    </parameter>
    <parameter type="integer" name="residuesperline">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[50]]></default>
      <information><![CDATA[Number of residues to be displayed on each
                  line]]></information>
      <relations><![CDATA[EDAM_data:1249 Sequence length]]></relations>
    </parameter>
    <parameter type="outfile" name="outfile">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[banana.profile]]></default>
      <knowntype><![CDATA[banana output]]></knowntype>
      <nullok><![CDATA[Y]]></nullok>
      <relations><![CDATA[EDAM_data:0912 Nucleic acid property]]></relations>
    </parameter>
  </section>
</application>
