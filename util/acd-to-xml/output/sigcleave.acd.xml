<?xml version="1.0"?>
<application id="sigcleave">
  <metadata>
    <documentation><![CDATA[Report on signal cleavage sites in a protein sequence]]></documentation>
    <groups><![CDATA[Protein:Functional sites, Protein:Motifs]]></groups>
    <relations><![CDATA[EDAM_topic:0748 Protein sites and features]]></relations>
    <relations><![CDATA[EDAM_topic:0158 Sequence motifs]]></relations>
    <relations><![CDATA[EDAM_topic:0140 Protein targeting and localization]]></relations>
    <relations><![CDATA[EDAM_operation:0418 Protein signal peptide detection]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[PureProtein]]></type>
      <relations><![CDATA[EDAM_data:2886 Sequence record (protein)]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="float" name="minweight">
      <standard><![CDATA[Y]]></standard>
      <minimum><![CDATA[0.]]></minimum>
      <maximum><![CDATA[100.]]></maximum>
      <default><![CDATA[3.5]]></default>
      <information><![CDATA[Minimum weight]]></information>
      <help><![CDATA[Minimum scoring weight value for the predicted cleavage
           site]]></help>
      <relations><![CDATA[EDAM_data:2146 Threshold]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="prokaryote">
      <additional><![CDATA[Y]]></additional>
      <help><![CDATA[Specifies the sequence is prokaryotic and changes the
           default scoring data file name]]></help>
      <information><![CDATA[Use prokaryotic cleavage data]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="report" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <rextension><![CDATA[sig]]></rextension>
      <rformat><![CDATA[motif]]></rformat>
      <multiple><![CDATA[Y]]></multiple>
      <taglist><![CDATA[str:mature_peptide]]></taglist>
      <relations><![CDATA[EDAM_data:1322 Protein features (signal peptides)]]></relations>
    </parameter>
  </section>
</application>
