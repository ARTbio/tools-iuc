<?xml version="1.0"?>
<application id="wordfinder">
  <metadata>
    <documentation><![CDATA[Match large sequences against one or more
                  other sequences]]></documentation>
    <groups><![CDATA[Alignment:Local]]></groups>
    <gui><![CDATA[yes]]></gui>
    <batch><![CDATA[yes]]></batch>
    <cpu><![CDATA[medium]]></cpu>
    <relations><![CDATA[EDAM_topic:0182 Sequence alignment]]></relations>
    <relations><![CDATA[EDAM_operation:0288 Sequence word comparison]]></relations>
    <relations><![CDATA[EDAM_operation:0493 Pairwise sequence alignment construction
    	       (local)]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqset" name="asequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[any]]></type>
      <aligned><![CDATA[N]]></aligned>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="seqall" name="bsequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[@($(acdprotein) ? stopprotein : nucleotide)]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
    <parameter type="matrixf" name="datafile">
      <additional><![CDATA[Y]]></additional>
      <information><![CDATA[Matrix file]]></information>
      <protein><![CDATA[$(acdprotein)]]></protein>
      <help><![CDATA[This is the scoring matrix file used when comparing
           sequences. By default it is the file 'EBLOSUM62' (for proteins) or
           the file 'EDNAFULL' (for nucleic sequences). These files are
           found in the 'data' directory of the EMBOSS installation.]]></help>
      <relations><![CDATA[EDAM_data:0874 Comparison matrix]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="float" name="gapopen">
      <standard><![CDATA[Y]]></standard>
      <information><![CDATA[Gap opening penalty]]></information>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[1000.0]]></maximum>
      <default><![CDATA[@($(acdprotein)? 30.0 : 30.0)]]></default>
      <expected><![CDATA[10.0 for any sequence type]]></expected>
      <relations><![CDATA[EDAM_data:1397 Gap opening penalty]]></relations>
    </parameter>
    <parameter type="float" name="gapextend">
      <standard><![CDATA[Y]]></standard>
      <information><![CDATA[Gap extension penalty]]></information>
      <minimum><![CDATA[0.0]]></minimum>
      <maximum><![CDATA[10.0]]></maximum>
      <default><![CDATA[@($(acdprotein)? 1.5 : 1.5)]]></default>
      <expected><![CDATA[0.5 for any sequence type]]></expected>
      <relations><![CDATA[EDAM_data:1398 Gap extension penalty]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="width">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[16]]></default>
      <minimum><![CDATA[1]]></minimum>
      <information><![CDATA[Alignment width]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="integer" name="wordlen">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[6]]></default>
      <minimum><![CDATA[3]]></minimum>
      <information><![CDATA[Word length for initial matching]]></information>
      <relations><![CDATA[EDAM_data:1250 Word size]]></relations>
    </parameter>
    <parameter type="integer" name="limitmatch">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Maximum match score (zero for no limit)]]></information>
      <relations><![CDATA[EDAM_data:1401 Match reward score]]></relations>
    </parameter>
    <parameter type="integer" name="limitalign">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Maximum alignment length (zero for no limit)]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="integer" name="lowmatch">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Minimum match score (zero for no limit)]]></information>
      <relations><![CDATA[EDAM_data:1401 Match reward score]]></relations>
    </parameter>
    <parameter type="integer" name="lowalign">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[0]]></minimum>
      <information><![CDATA[Minimum alignment length (zero for no limit)]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="align" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <aformat><![CDATA[simple]]></aformat>
      <minseqs><![CDATA[2]]></minseqs>
      <maxseqs><![CDATA[2]]></maxseqs>
      <relations><![CDATA[EDAM_data:1381 Sequence alignment (pair)]]></relations>
    </parameter>
    <parameter type="outfile" name="errfile">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[wordfinder.error]]></default>
      <help><![CDATA[Error file to be written to]]></help>
      <knowntype><![CDATA[wordfinder error]]></knowntype>
      <information><![CDATA[Program wordfinder errors output file]]></information>
      <relations><![CDATA[EDAM_data:1678 Tool log]]></relations>
    </parameter>
  </section>
</application>
