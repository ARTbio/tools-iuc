<?xml version="1.0"?>
<application id="cutgextract">
  <metadata>
    <documentation><![CDATA[Extract codon usage tables from CUTG database]]></documentation>
    <groups><![CDATA[Utils:Database creation]]></groups>
    <gui><![CDATA[no]]></gui>
    <batch><![CDATA[no]]></batch>
    <cpu><![CDATA[medium]]></cpu>
    <relations><![CDATA[EDAM_topic:0107 Codon usage analysis]]></relations>
    <relations><![CDATA[EDAM_topic:0091 Data handling]]></relations>
    <relations><![CDATA[EDAM_operation:1812 Data loading]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="dirlist" name="directory">
      <parameter><![CDATA[Y]]></parameter>
      <information><![CDATA[Codon usage directories]]></information>
      <knowntype><![CDATA[codon usage]]></knowntype>
      <relations><![CDATA[EDAM_data:1597 Codon usage table]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="string" name="release">
      <default><![CDATA[]]></default>
      <information><![CDATA[CUTG release]]></information>
      <knowntype><![CDATA[database release]]></knowntype>
      <relations><![CDATA[EDAM_data:1670 Database version information]]></relations>
    </parameter>
    <parameter type="string" name="wildspec">
      <default><![CDATA[*.codon]]></default>
      <information><![CDATA[Type of codon file]]></information>
      <knowntype><![CDATA[filename wildcard]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1050 File name]]></relations>
    </parameter>
    <parameter type="string" name="species">
      <default><![CDATA[*]]></default>
      <information><![CDATA[Species wildcard]]></information>
      <knowntype><![CDATA[wildcard string]]></knowntype>
      <relations><![CDATA[EDAM_data:0968 Keyword]]></relations>
    </parameter>
    <parameter type="boolean" name="allrecords">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Include all records, including those with
                  warnings]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[log]]></knowntype>
      <information><![CDATA[General log output file]]></information>
      <relations><![CDATA[EDAM_data:1678 Tool log]]></relations>
    </parameter>
    <parameter type="string" name="filename">
      <default><![CDATA[]]></default>
      <information><![CDATA[Single output filename]]></information>
      <help><![CDATA[Single output filename to override automatic one file for
           each species. Leave blank to generate the filenames. Specify a
           filename to combine several species into one file.]]></help>
      <knowntype><![CDATA[output filename]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1050 File name]]></relations>
    </parameter>
  </section>
</application>
