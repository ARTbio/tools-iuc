<?xml version="1.0"?>
<application id="distmat">
  <metadata>
    <documentation><![CDATA[Create a distance matrix from a multiple sequence alignment]]></documentation>
    <groups><![CDATA[Phylogeny:Molecular sequence]]></groups>
    <relations><![CDATA[EDAM_topic:0159 Sequence comparison]]></relations>
    <relations><![CDATA[EDAM_topic:0084 Phylogenetics]]></relations>
    <relations><![CDATA[EDAM_operation:0289 Sequence distance matrix generation]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqset" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[gapany]]></type>
      <aligned><![CDATA[Y]]></aligned>
      <minseqs><![CDATA[2]]></minseqs>
      <help><![CDATA[File containing a sequence alignment.]]></help>
      <relations><![CDATA[EDAM_data:0863 Sequence alignment]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="list" name="nucmethod">
      <standard><![CDATA[@(!$(acdprotein))]]></standard>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[1]]></maximum>
      <header><![CDATA[Multiple substitution correction methods for
             nucleotides]]></header>
      <values><![CDATA[0:Uncorrected; 1:Jukes-Cantor; 2:Kimura; 3:Tamura;
             4:Tajima-Nei; 5:Jin-Nei Gamma]]></values>
      <delimiter><![CDATA[;]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Method to use]]></information>
      <help><![CDATA[Multiple substitution correction methods for nucleotides.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="list" name="protmethod">
      <standard><![CDATA[$(acdprotein)]]></standard>
      <default><![CDATA[0]]></default>
      <minimum><![CDATA[1]]></minimum>
      <maximum><![CDATA[1]]></maximum>
      <header><![CDATA[Multiple substitution correction methods for proteins]]></header>
      <values><![CDATA[0:Uncorrected; 1:Jukes-Cantor; 2:Kimura Protein;]]></values>
      <delimiter><![CDATA[;]]></delimiter>
      <codedelimiter><![CDATA[:]]></codedelimiter>
      <information><![CDATA[Method to use]]></information>
      <help><![CDATA[Multiple substitution correction methods for proteins.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="ambiguous">
      <additional><![CDATA[@(@(@(!$(acdprotein)) & @($(nucmethod)==1)) |
                 @($(acdprotein) & @($(protmethod)!=2)))]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Use the ambiguous codes in the calculation.]]></information>
      <help><![CDATA[Option to use the ambiguous codes in the calculation of the
           Jukes-Cantor method or if the sequences are proteins.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="float" name="gapweight">
      <additional><![CDATA[@(@(@($(nucmethod)<2) & @(!$(acdprotein))) |
                 @($(acdprotein) & @($(protmethod)!=2)))]]></additional>
      <information><![CDATA[Weight given to gaps]]></information>
      <default><![CDATA[0.]]></default>
      <help><![CDATA[Option to weight gaps in the uncorrected (nucleotide) and
           Jukes-Cantor distance methods.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="integer" name="position">
      <additional><![CDATA[@(@(!$(acdprotein)) & @($(nucmethod)!=4))]]></additional>
      <default><![CDATA[123]]></default>
      <information><![CDATA[Base position to analyse]]></information>
      <help><![CDATA[Choose base positions to analyse in each codon i.e. 123
           (all bases), 12 (the first two bases), 1, 2, or 3 individual
           bases.]]></help>
      <relations><![CDATA[EDAM_data:1016 Sequence position]]></relations>
    </parameter>
    <parameter type="boolean" name="calculatea">
      <additional><![CDATA[@(@(!$(acdprotein)) & @($(nucmethod)==5))]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Calculate the nucleotide Jin-Nei parameter
                  'a']]></information>
      <help><![CDATA[This will force the calculation of parameter 'a' in the
           Jin-Nei Gamma distance calculation, otherwise the default is 1.0
           (see -parametera option).]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="float" name="parametera">
      <additional><![CDATA[@(@(!$(calculatea)) & @($(nucmethod)==5))]]></additional>
      <default><![CDATA[1.0]]></default>
      <information><![CDATA[Nucleotide Jin-Nei parameter 'a']]></information>
      <help><![CDATA[User defined parameter 'a' to be use in the Jin-Nei Gamma
           distance calculation. The suggested value to be used is 1.0 (Jin
           et al.) and this is the default.]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[distance matrix]]></knowntype>
      <information><![CDATA[Phylip distance matrix output file]]></information>
      <relations><![CDATA[EDAM_data:0870 Sequence distance matrix]]></relations>
    </parameter>
  </section>
</application>
