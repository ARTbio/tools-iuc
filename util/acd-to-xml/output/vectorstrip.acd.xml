<?xml version="1.0"?>
<application id="vectorstrip">
  <metadata>
    <documentation><![CDATA[Remove vectors from the ends of nucleotide sequence(s)]]></documentation>
    <groups><![CDATA[Edit]]></groups>
    <relations><![CDATA[EDAM_topic:0091 Data handling]]></relations>
    <relations><![CDATA[EDAM_operation:0369 Sequence cutting]]></relations>
    <relations><![CDATA[EDAM_operation:0266 Vector sequence detection]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[dna]]></type>
      <relations><![CDATA[EDAM_data:2887 Sequence record (nucleic acid)]]></relations>
    </parameter>
    <section id="vectorfilesection">
      <metadata>
        <information><![CDATA[Vector input options]]></information>
        <type><![CDATA[frame]]></type>
      </metadata>
      <parameter type="toggle" name="readfile">
        <parameter><![CDATA[Y]]></parameter>
        <default><![CDATA[Y]]></default>
        <information><![CDATA[Are your vector sequences in a file?]]></information>
        <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
      </parameter>
      <parameter type="infile" name="vectorsfile">
        <standard><![CDATA[$(readfile)]]></standard>
        <nullok><![CDATA[Y]]></nullok>
        <default><![CDATA[]]></default>
        <information><![CDATA[Cloning vector definition file (optional)]]></information>
        <knowntype><![CDATA[vector]]></knowntype>
        <relations><![CDATA[EDAM_data:0850 Sequence set]]></relations>
      </parameter>
    </section>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="mismatch">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[10]]></default>
      <information><![CDATA[Max allowed % mismatch]]></information>
      <relations><![CDATA[EDAM_data:2146 Threshold]]></relations>
    </parameter>
    <parameter type="boolean" name="besthits">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[Y]]></default>
      <information><![CDATA[Show only the best hits (minimise
                  mismatches)?]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="string" name="alinker">
      <standard><![CDATA[@(!$(readfile))]]></standard>
      <information><![CDATA[The 5' sequence]]></information>
      <knowntype><![CDATA[nucleotide sequence]]></knowntype>
      <relations><![CDATA[EDAM_format:1210 pure nucleotide]]></relations>
    </parameter>
    <parameter type="string" name="blinker">
      <standard><![CDATA[@(!$(readfile))]]></standard>
      <information><![CDATA[The 3' sequence]]></information>
      <knowntype><![CDATA[nucleotide sequence]]></knowntype>
      <relations><![CDATA[EDAM_format:1210 pure nucleotide]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="allsequences">
      <additional><![CDATA[Y]]></additional>
      <information><![CDATA[Show all sequences in output]]></information>
      <default><![CDATA[N]]></default>
      <outputmodifier><![CDATA[Y]]></outputmodifier>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="outfile" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <knowntype><![CDATA[vectorstrip output]]></knowntype>
      <relations><![CDATA[EDAM_data:1678 Tool log]]></relations>
    </parameter>
    <parameter type="seqoutall" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <relations><![CDATA[EDAM_data:2887 Sequence record (nucleic acid)]]></relations>
    </parameter>
  </section>
</application>
