<?xml version="1.0"?>
<application id="silent">
  <metadata>
    <documentation><![CDATA[Find restriction sites to insert (mutate)
                  with no translation change]]></documentation>
    <groups><![CDATA[Nucleic:Restriction]]></groups>
    <relations><![CDATA[EDAM_topic:0100 Nucleic acid restriction]]></relations>
    <relations><![CDATA[EDAM_operation:0431 Restriction site recognition]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="sequence" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[dna]]></type>
      <relations><![CDATA[EDAM_data:2887 Sequence record (nucleic acid)]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="string" name="enzymes">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[all]]></default>
      <information><![CDATA[Comma separated enzyme list]]></information>
      <knowntype><![CDATA[enzyme list]]></knowntype>
      <relations><![CDATA[EDAM_identifier:1013 Restriction enzyme name]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="sshow">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Display untranslated sequence]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="tshow">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Display translated sequence]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="allmut">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Display all mutations]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="report" name="outfile">
      <parameter><![CDATA[Y]]></parameter>
      <rformat><![CDATA[table]]></rformat>
      <taglist><![CDATA[str:enzyme=EnzymeName str:rspattern=RS-Pattern
    	      int:baseposn=Base-Posn rstr:aa=AAs
              rstr:silent=Silent rstr:mutation=Mutation]]></taglist>
      <rscoreshow><![CDATA[N]]></rscoreshow>
      <relations><![CDATA[EDAM_data:1305 Nucleic acid features (restriction sites)]]></relations>
    </parameter>
  </section>
</application>
