<?xml version="1.0"?>
<application id="hmoment">
  <metadata>
    <documentation><![CDATA[Calculate and plot hydrophobic moment for protein sequence(s)]]></documentation>
    <groups><![CDATA[Protein:Properties]]></groups>
    <relations><![CDATA[EDAM_topic:0137 Protein hydropathy]]></relations>
    <relations><![CDATA[EDAM_operation:0407 Protein hydrophobic moment plotting]]></relations>
    <relations><![CDATA[EDAM_operation:0401 Protein hydropathy calculation (from sequence)]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqall" name="seqall">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[protein]]></type>
      <relations><![CDATA[EDAM_data:2886 Sequence record (protein)]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="integer" name="window">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[10]]></default>
      <information><![CDATA[Window]]></information>
      <relations><![CDATA[EDAM_data:1251 Window size]]></relations>
    </parameter>
    <parameter type="integer" name="aangle">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[100]]></default>
      <information><![CDATA[Alpha helix angle (degrees)]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="integer" name="bangle">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[160]]></default>
      <information><![CDATA[Beta sheet angle (degrees)]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="advanced">
    <metadata>
      <information><![CDATA[Advanced section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="float" name="baseline">
      <default><![CDATA[0.35]]></default>
      <information><![CDATA[Graph marker line]]></information>
      <relations><![CDATA[EDAM_data:2152 Rendering parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="toggle" name="plot">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Produce graphic]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="boolean" name="double">
      <default><![CDATA[N]]></default>
      <information><![CDATA[Plot two graphs]]></information>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
    <parameter type="xygraph" name="graph">
      <standard><![CDATA[$(plot)]]></standard>
      <relations><![CDATA[EDAM_data:1520 Peptide hydrophobic moment]]></relations>
      <sequence><![CDATA[Y]]></sequence>
    </parameter>
    <parameter type="outfile" name="outfile">
      <standard><![CDATA[@(!$(plot))]]></standard>
      <knowntype><![CDATA[hmoment output]]></knowntype>
      <nullok><![CDATA[$(plot)]]></nullok>
      <nulldefault><![CDATA[$(plot)]]></nulldefault>
      <relations><![CDATA[EDAM_data:1520 Peptide hydrophobic moment]]></relations>
    </parameter>
  </section>
</application>
