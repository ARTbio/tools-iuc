<?xml version="1.0"?>
<application id="extractseq">
  <metadata>
    <documentation><![CDATA[Extract regions from a sequence]]></documentation>
    <groups><![CDATA[Edit]]></groups>
    <relations><![CDATA[EDAM_topic:0091 Data handling]]></relations>
    <relations><![CDATA[EDAM_operation:0369 Sequence cutting]]></relations>
  </metadata>
  <section id="input">
    <metadata>
      <information><![CDATA[Input section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="sequence" name="sequence">
      <parameter><![CDATA[Y]]></parameter>
      <type><![CDATA[any]]></type>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
  <section id="required">
    <metadata>
      <information><![CDATA[Required section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="range" name="regions">
      <standard><![CDATA[Y]]></standard>
      <default><![CDATA[$(sequence.begin)-$(sequence.end)]]></default>
      <information><![CDATA[Regions to extract (eg: 4-57,78-94)]]></information>
      <help><![CDATA[Regions to extract. \
           A set of regions is specified by a set of pairs of positions. \
           The positions are integers. \
           They are separated by any non-digit, non-alpha character. \
           Examples of region specifications are: \
           24-45, 56-78 \
           1:45, 67=99;765..888 \
           1,5,8,10,23,45,57,99]]></help>
      <expected><![CDATA[Whole sequence]]></expected>
      <relations><![CDATA[EDAM_data:1017 Sequence range]]></relations>
    </parameter>
  </section>
  <section id="additional">
    <metadata>
      <information><![CDATA[Additional section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="boolean" name="separate">
      <additional><![CDATA[Y]]></additional>
      <default><![CDATA[N]]></default>
      <information><![CDATA[Write regions to separate sequences]]></information>
      <help><![CDATA[If this is set true then each specified region is written
           out as a separate sequence. The name of the sequence is created
           from the name of the original sequence with the start and end
           positions of the range appended with underscore characters between
           them, eg: XYZ region 2 to 34 is written as: XYZ_2_34]]></help>
      <relations><![CDATA[EDAM_data:2527 Parameter]]></relations>
    </parameter>
  </section>
  <section id="output">
    <metadata>
      <information><![CDATA[Output section]]></information>
      <type><![CDATA[page]]></type>
    </metadata>
    <parameter type="seqoutall" name="outseq">
      <parameter><![CDATA[Y]]></parameter>
      <relations><![CDATA[EDAM_data:0849 Sequence record]]></relations>
    </parameter>
  </section>
</application>
