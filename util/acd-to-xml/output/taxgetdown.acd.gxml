<?xml version='1.0' encoding='UTF-8'?>
<tool id="taxgetdown" name="taxgetdown" version="6.6.0">
  <description>Get descendants of taxon(s)</description>
  <macros>
    <token name="@EXECUTABLE@">taxgetdown</token>
    <import>macros.xml</import>
  </macros>
  <expand macro="stdio"/>
  <expand macro="requirements"/>
  <command>taxgetdown

#if $param_taxgetdown_version:
  -taxgetdown:version     "$param_taxgetdown_version"
#end if
#if $param_input_taxons:
  -input:taxons     "$param_input_taxons"
#end if
#if $param_outfile:
  -outfile $param_outfile
#end if
</command>
  <inputs>
    <param name="param_taxgetdown_version" type="text" size="30" value="6.6.0" label="Version of the tool that generated this parameters file" help="(-version) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
    <param name="param_input_taxons" type="text" size="30" label="taxons parameter" help="(-taxons) ">
      <sanitizer>
        <valid initial="string.printable">
          <remove value="'"/>
          <remove value="&quot;"/>
        </valid>
      </sanitizer>
    </param>
  </inputs>
  <outputs>
    <data name="param_outfile" format="data"/>
  </outputs>
  <help>**What it does**

</help>
  <expand macro="references"/>
</tool>
